    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceFolder}/build/main.js",
            "preLaunchTask": "${defaultBuildTask}",
            "console": "internalConsole",
            "outputCapture": "std",
            "args": [
                "-codec:0", "h264", 
                "-codec:1", "dca", 
                "-ss", "0", 
                "-noaccurate_seek", 
                "-analyzeduration", "20000000", 
                "-probesize", "20000000", 
                "-i", "/data/movies/Avatar.mkv", 
                "-filter_complex", "[0:1] aresample=async=1:ocl='stereo':rematrix_maxval=0.000000dB:osr=48000[0]", 
                "-map", "0:0", 
                "-codec:0", "copy", 
                "-map", "[0]", 
                "-metadata:s:1", "language=eng", 
                "-codec:1", "aac", 
                "-b:1", "256k", 
                "-f", "dash", 
                "-seg_duration", "5", 
                "-init_seg_name", "init-stream$RepresentationID$.m4s", 
                "-media_seg_name", "chunk-stream$RepresentationID$-$Number%05d$.m4s", 
                "-window_size", "5", 
                "-delete_removed", "false", 
                "-skip_to_segment", "1", 
                "-time_delta", "0.0625", 
                "-manifest_name", "http://127.0.0.1:32400/video/:/transcode/session/g2zskpjgszpzo8t42b0bhc07/bd414719-c1a1-4e91-a457-b48bf598c383/manifest?X-Plex-Http-Pipeline=infinite", 
                "-avoid_negative_ts", "disabled", 
                "-map_metadata", "-1", 
                "-map_chapters", "-1", 
                "dash", 
                "-start_at_zero", 
                "-copyts", 
                "-vsync", "cfr", 
                "-y", 
                "-nostats", 
                "-loglevel", "quiet", 
                "-loglevel_plex", "error", 
                "-progressurl", "http://127.0.0.1:32400/video/:/transcode/session/g2zskpjgszpzo8t42b0bhc07/bd414719-c1a1-4e91-a457-b48bf598c383/progress"
            ],
            "env": {
                "TRANSCODER_URL": "http://localhost:34800/api/transcode"
            }
        }
    ]
}